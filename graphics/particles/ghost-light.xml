<?xml version="1.0"?>
<!-- A flickering lamp for ghosts. -->
<effect>
  <particle position-x="0" position-y="0" position-z="0">
    <emitter>
      <property name="position-y" min="-33" max="-34"/>
      <property name="position-x" min="-10" max="-11"/>
      <property name="position-z" min="0" max="0"/>
      <property name="fade-in" value="100"/>
      <property name="fade-out" value="100"/>
      <property name="lifetime" value="200"/>
      <property name="output" min="0" max="1"/>
      <property name="output-pause" min="3" max="7"/>
      <property name="image" value="graphics/particles/light-large-lite.png|W:#ffe691"/>
    </emitter>
    <emitter>
      <property name="position-y" min="-33" max="-34"/>
      <property name="position-x" min="-10" max="-11"/>
      <property name="position-z" min="0" max="0"/>
      <property name="fade-in" value="10"/>
      <property name="fade-out" value="10"/>
      <property name="lifetime" value="20"/>
      <property name="image" value="graphics/particles/light-large-lite.png|W:#ffe691"/>
    </emitter>
  </particle>
</effect>
